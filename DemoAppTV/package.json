    "@snowplow/react-native-tracker": "file:.."
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "babel-jest": "^29.0.3",
    "eslint": "8.23.1",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "react-test-renderer": "18.2.0",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
