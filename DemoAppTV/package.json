{
  "name": "demoapptv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "tvos": "react-native run-ios --simulator \"Apple TV\" --scheme \"DemoAppTV-tvOS\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pods": "cd ios && rimraf Pods && pod install",
